{{#each this}}
<a class="gallery__item" href="{{largeImageURL}}" target="_blank" rel="noopener noreferrer">
<div class="photo-card">
<img class="card__image " src="{{webformatURL}}" alt="{{tags}}" loading="lazy"/>
<div class="info">
<p class="info_item"><b>Likes: </b>{{likes}}</p>
<p class="info_item"><b>Views: </b>{{views}}</p>
<p class="info_item"><b>Comments: </b>{{comments}}</p>
<p class="info_item"><b>Downloads: </b>{{downloads}}</p>
</div>
</div>
</a>
{{/each}}